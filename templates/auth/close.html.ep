    <body>
        <script type="text/javascript">
            window.close();
            <% if(defined($is_error)) { %>
                window.opener.location.href = '<% session('auth_return') %>?kcauth=error&error=<% $error %>';
            <% } else { %>
                window.opener.location.href = window.opener.location.href + '?kcauth=ok';
            <% } %>
        </script>
    </body>
</html>
